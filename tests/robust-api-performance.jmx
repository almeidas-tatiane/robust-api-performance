            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def filePath = &quot;${BASE_DIR}/created_users.csv&quot; 

def user = vars.get(&quot;username&quot;)
def pass = vars.get(&quot;password&quot;)

def file = new File(filePath)

// If the file doesn&apos;t exist, create one with a username and password header 
if (!file.exists()) {
    file &lt;&lt; &quot;username,password\n&quot;
}

// Add line with user and password created
file &lt;&lt; &quot;${user},${pass}\n&quot;
</stringProp>
