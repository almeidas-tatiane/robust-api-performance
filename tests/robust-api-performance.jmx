        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler - JMeter Props" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">sampleResult.setIgnore();

String environment = props.get(&quot;environment&quot;).toUpperCase();

// Mapping values
def envConfig = [
    &quot;LOCAL&quot;: [
        &quot;host&quot;: vars.get(&quot;hostLocal&quot;),
        &quot;port&quot;: vars.get(&quot;portLocal&quot;),
        &quot;protocol&quot;: vars.get(&quot;protocolLocal&quot;)
    ],
    &quot;AWS&quot;: [
        &quot;host&quot;: vars.get(&quot;hostAWS&quot;),
        &quot;port&quot;: vars.get(&quot;portAWS&quot;),
        &quot;protocol&quot;: vars.get(&quot;protocolAWS&quot;)
    ]
];

if (!envConfig.containsKey(environment)) {
    throw new IllegalArgumentException(&quot;Environment invalido: &quot; + environment);
}

// Define all the properties together 
envConfig[environment].each { key, value -&gt;
    props.put(key, value);
    log.info(&quot;${key}: ${value}&quot;);
}

//Example how to execute the script in command line
//jmeter.bat -n -t robust-api-performance.jmx -l log.csv -e -o HTMLReport -Jenvironment=AWS</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
